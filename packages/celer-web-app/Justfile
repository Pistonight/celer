install:
    npm install

ci:
    npm ci

lint:
    pylint scripts/validateimport.py
    python3 scripts/validateimport.py src
    npm run lint

lint-fix:
    npm run lint -- --fix

build:
    npm run build

watch:
    npm run start

release: build
    mkdir -p ../../release
    cp -R -T build ../../release/celer-web-app

clean:
    rm -rf build

nuke: clean
    rm -rf node_modules
